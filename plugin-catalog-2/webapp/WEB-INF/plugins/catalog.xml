<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<plug-in>
    <name>catalog</name>
    <class>fr.paris.lutece.portal.service.plugin.PluginDefaultImplementation</class>
    <version>1.0.0-SNAPSHOT</version>
    <documentation></documentation>
    <installation></installation>
    <changes></changes>
    <user-guide></user-guide>
    <description>catalog.plugin.description</description>
    <provider>catalog.plugin.provider</provider>
    <provider-url>https://dev.lutece.paris.fr/</provider-url>
    <icon-url>images/admin/skin/feature_default_icon.png</icon-url>
    <copyright>Copyright (c) 2021</copyright>
    <db-pool-required>1</db-pool-required>
    
    <core-version-dependency>
        <min-core-version>4.1.0</min-core-version>
        <max-core-version/>
    </core-version-dependency>

    <!-- Admin features parameters -->
    <admin-features>
        <admin-feature>
            <feature-id>CATALOG_MANAGEMENT</feature-id>
            <feature-title>catalog.adminFeature.Update.name</feature-title>
            <feature-description>catalog.adminFeature.Update.description</feature-description>
            <feature-level>0</feature-level>
            <feature-url>jsp/admin/plugins/catalog/ManageCatalogs.jsp</feature-url>
            <feature-icon-url></feature-icon-url>
        </admin-feature>
    </admin-features>

    <!-- XPages parameters -->
    <applications>
        <application>
            <application-id>catalog</application-id>
            <application-class>fr.paris.lutece.plugins.catalog.web.CatalogXPage</application-class>
        </application>
    </applications>

    <!-- Portlet parameters -->
    <portlets>
    </portlets>
</plug-in>